name = AdminLTE
description = AdminLTE is a fully responsive theme for admin panels. It is rich with features and simple to use. Full support for Bootstrap 3.x.
core = 7.x

;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[sidebar_first]  = 'Primary'
regions[sidebar_second] = 'Secondary'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'

;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/style.css
stylesheets[all][] = css/bootstrap.min.css
stylesheets[all][] = css/font-awesome.min.css
stylesheets[all][] = css/ionicons.min.css
stylesheets[all][] = css/morris/morris.css
stylesheets[all][] = css/jvectormap/jquery-jvectormap-1.2.2.css
stylesheets[all][] = css/fullcalendar/fullcalendar.css
stylesheets[all][] = css/daterangepicker/daterangepicker-bs3.css
stylesheets[all][] = css/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css
stylesheets[all][] = css/AdminLTE.css

;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;;

scripts[] = '/js/jquery-ui-1.10.3.min.js'
scripts[] = '/js/bootstrap.min.js'
scripts[] = '/js/plugins/morris/morris.min.js'
scripts[] = '/js/plugins/sparkline/jquery.sparkline.min.js'
scripts[] = '/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'
scripts[] = '/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js'
scripts[] = '/js/plugins/fullcalendar/fullcalendar.min.js'
scripts[] = '/js/plugins/jqueryKnob/jquery.knob.js'
scripts[] = '/js/plugins/daterangepicker/daterangepicker.js'
scripts[] = '/js/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js'
scripts[] = '/js/plugins/iCheck/icheck.min.js'
scripts[] = '/js/AdminLTE/app.js'
scripts[] = '/js/AdminLTE/dashboard.js'
